#include<stdio.h>
int main()
{
int age;
int number;
char name[50];
char school[50];
char course[50];
char year[50];
	
printf("Full Name: ");
gets( name );
	
printf("School:");
gets( school );

printf("Course:");
gets( course );
				
printf("Year:");
gets( year );
	
printf("Age:");
scanf("%d", &age);
	
printf("Contact Number:");
scanf("%d", &number);
	
	
printf("\nHello, I am %s. I am %d years old. My Contact Number is %d. I'm Studying at %s in the Course of %s. I am in my %s.", name, age,number, school, course, year);

return 0;
	
}
